<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>LoadServlet</servlet-name>
        <servlet-class>jav.LoadServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SaveServlet</servlet-name>
        <servlet-class>jav.SaveServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SubscribeServlet</servlet-name>
        <servlet-class>jav.SubscribeServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CallbackServlet</servlet-name>
        <servlet-class>jav.CallbackServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SaveTemplate</servlet-name>
        <servlet-class>jav.SaveTemplate</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ChangeStateServlet</servlet-name>
        <servlet-class>jav.ChangeStateServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CalendarServlet</servlet-name>
        <servlet-class>jav.CalendarServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NewServlet</servlet-name>
        <servlet-class>jav.NewServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>getDocs</servlet-name>
        <servlet-class>jav.GetDocs</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PrendiDocs</servlet-name>
        <servlet-class>jav.PrendiDocs</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>IdentityChecker</servlet-name>
        <servlet-class>jav.IdentityChecker</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CheckId</servlet-name>
        <servlet-class>jav.CheckId</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoadServlet</servlet-name>
        <url-pattern>/LoadServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SubscribeServlet</servlet-name>
        <url-pattern>/SubscribeServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SaveServlet</servlet-name>
        <url-pattern>/SaveServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CallbackServlet</servlet-name>
        <url-pattern>/CallbackServlet/subscribe/pippo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SaveTemplate</servlet-name>
        <url-pattern>/SaveTemplate</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ChangeStateServlet</servlet-name>
        <url-pattern>/ChangeStateServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CalendarServlet</servlet-name>
        <url-pattern>/CalendarServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NewServlet</servlet-name>
        <url-pattern>/NewServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>getDocs</servlet-name>
        <url-pattern>/getDocs</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PrendiDocs</servlet-name>
        <url-pattern>/PrendiDocs</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>IdentityChecker</servlet-name>
        <url-pattern>/IdentityChecker</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CheckId</servlet-name>
        <url-pattern>/CheckId</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
