<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>SubscribeServlet</servlet-name>
        <servlet-class>jav.SubscribeServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NotifCallbackServlet</servlet-name>
        <servlet-class>jav.NotifCallbackServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PrendiDocs</servlet-name>
        <servlet-class>jav.PrendiDocs</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>IdentityChecker</servlet-name>
        <servlet-class>jav.IdentityChecker</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SubscribeServlet</servlet-name>
        <url-pattern>/SubscribeServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NotifCallbackServlet</servlet-name>
        <url-pattern>/NotifCallbackServlet/subscribe/pippo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PrendiDocs</servlet-name>
        <url-pattern>/PrendiDocs</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>IdentityChecker</servlet-name>
        <url-pattern>/IdentityChecker</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
